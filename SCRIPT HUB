local function LBTHB_fake_script() -- UNIVERSALAIMBOT.LocalScript 
	local script = Instance.new('LocalScript', UNIVERSALAIMBOT)

	script.Parent.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet"https://raw.githubusercontent.com/Wheeleee/AIMWARE/main/Startup")()
	end)
end
coroutine.wrap(LBTHB_fake_script)()
local function VIHW_fake_script() -- CHOPPAHUB.LocalScript 
	local script = Instance.new('LocalScript', CHOPPAHUB)

	function onKeyPress(inputObject, gameProcessedEvent)
		if not gameProcessedEvent then
	    if inputObject.KeyCode == Enum.KeyCode.RightAlt  then 
			script.Parent.Frame.Visible = not script.Parent.Frame.Visible
			end
		end
	end
	game:GetService("UserInputService").InputBegan:connect(onKeyPress)
end
coroutine.wrap(VIHW_fake_script)()
