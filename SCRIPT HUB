-- Scripts:

local function XRDSZ_fake_script() -- discord.LocalScript 
	local script = Instance.new('LocalScript', discord)

	script.Parent.MouseButton1Click:Connect(function()
		loadstring(discord.gg/Nrb8rf9P)
	end)
end
coroutine.wrap(XRDSZ_fake_script)()
local function FBVEB_fake_script() -- PHANTOMFORCES.LocalScript 
	local script = Instance.new('LocalScript', PHANTOMFORCES)

	script.Parent.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Averiias/purple-haze-pf/main/loader.lua"))()
	end)
end
coroutine.wrap(FBVEB_fake_script)()
local function RDPBIR_fake_script() -- Jailbird.LocalScript 
	local script = Instance.new('LocalScript', Jailbird)

	script.Parent.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/1iseo/rbx/main/jailbird/jailbird.lua"))()
	end)
end
coroutine.wrap(RDPBIR_fake_script)()
local function SUDSF_fake_script() -- InfiniteYield.LocalScript 
	local script = Instance.new('LocalScript', InfiniteYield)

	script.Parent.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'),true))()
	end)
	
end
coroutine.wrap(SUDSF_fake_script)()
